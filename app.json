      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCok-ulL8N_UEHG3KbIc4rVfrN3Vrfju8A"
        }
      },
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    }
